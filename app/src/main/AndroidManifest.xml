<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="come.example.senasoft.trasmiautos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".vista.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".vista.MiPerfil"
            android:label="@string/title_activity_mi_perfil"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".vista.Servicios" />
        <activity android:name=".vista.Registro" />
        <activity android:name=".vista.AcercaDe" />
        <activity
            android:name=".vista.Perfil"
            android:label="@string/title_activity_perfil"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>